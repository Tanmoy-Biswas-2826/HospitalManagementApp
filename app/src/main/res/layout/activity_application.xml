<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".application">

    <LinearLayout
        android:layout_width="119dp"
        android:layout_height="600dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout2"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/cardiology" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Cardiology"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/dermatology" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Dermatology"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/generalmedicine" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="General_Medicine"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="83dp"
            android:layout_height="98dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/nephrology" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Nephrology"
            android:textAlignment="center"
            android:textSize="15sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="119dp"
        android:layout_height="600dp"
        android:layout_marginEnd="4dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout3"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout3"
        app:layout_constraintTop_toTopOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/criticalcare" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Criticalcare"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/endocrinology" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Endocrinology"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/gynaecology" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Gynaecology"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/optcichaimology" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Optcichaimology"
            android:textAlignment="center"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="119dp"
        android:layout_height="600dp"
        android:layout_marginEnd="16dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/dentistry" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Dentistry"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/gastrology" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Gastrology"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/infertility" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Infertility"
            android:textAlignment="center"
            android:textSize="15sp" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="83dp"
            android:layout_height="95dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:background="?android:attr/selectableItemBackground"
            android:scaleType="fitStart"
            app:srcCompat="@drawable/orthopaedic" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="Orthopaedic"
            android:textAlignment="center"
            android:textSize="15sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:fontFamily="serif"
        android:text="Select a Department"
        android:background="@color/sky_200"
        android:textAlignment="center"
        android:textColor="#020202"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6"
        tools:layout_editor_absoluteX="0dp" />

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_marginTop="50dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="1dp">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="79dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:scaleType="fitEnd"
            app:srcCompat="@drawable/loginlogo" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:text="ALIVE"
            android:textAlignment="viewStart"
            android:textSize="30sp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>